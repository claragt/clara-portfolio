<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <title>{{ renderData.title or title or metadata.title }}</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="theme-color" content="##ffffff">
  <meta name="format-detection" content="telephone=no">
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
  <meta name="apple-mobile-web-app-title" content="{{ renderData.title or title or metadata.title }}">
  <meta name="disabled-adaptations" content="watch">
  <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
  <link rel="apple-touch-icon" href="/static/img/apple-touch-icon.png" sizes="180x180">
  <link rel="manifest" href="/manifest.json" type="application/json" crossorigin="use-credentials">
  <!-- 
  <link rel="alternate" href="https://www.claragreo.com" hreflang="x-default">
  <link rel="canonical" href="https://www.claragreo.com/" itemprop="url">
  -->
  
  <!-- Open graph -->
  <meta property="og:site_name" content="{{ renderData.title or title or metadata.title }}">
  <meta property="og:title" content="{{ renderData.title or title or metadata.title }}">
  <meta property="og:description" content="{{ renderData.summary or metadata.summary or summary}}">
  <meta property="og:type" content="article">
  <meta property="og:image" content="{{ renderData.cover or cover }}">
  <meta property="og:image:width" content="1200">
  <meta property="og:image:height" content="630">
  
  <!-- Twitter -->
  <meta name="twitter:title" content="{{ renderData.title or title or metadata.title }}">
  <meta name="twitter:card" content="summary">
  <meta name="twitter:creator" content="@claragt">
  <meta name="twitter:site" content="@claragt">
  <meta name="twitter:widgets:csp" content="on">
  <meta name="twitter:image" content="{{ renderData.cover or cover }}">
  <meta name="twitter:description" content="{{ renderData.summary or metadata.summary or summary}}">
  
  <link rel="dns-prefetch" href="https://identity.netlify.com">
  <meta name="description" content="{{ metaDescription or renderData.metaDescription or metadata.description }}">
  <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Quicksand:wght@400;600">
  <script async src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>
  
  <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
  <link rel="manifest" href="/site.webmanifest">
  {% set css %}
    {% include "assets/css/layouts.css" %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>
  {% set js %}
    {% include "assets/js/inline.js" %}
  {% endset %}
  <script>{{ js | jsmin | safe }}</script>
</head>
